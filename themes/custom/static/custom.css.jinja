body {
  margin: 0;
  padding: 0;
  background-color: #ffffff;
  overflow: hidden;
}

h1 {
  font-size: 1.875rem;
  line-height: 2.25rem;
}

h2 {
  color: var(--tertiary);
  font-size: 1.25rem;
  line-height: 1.75rem;
}
h3 {
  font-size: 1.125rem;
  line-height: 1.75rem;
}

a {
  text-decoration: none;
  color: var(--tertiary);
}

a:hover {
  color: var(--accent);
}

a.headerlink {
  color: transparent;
}

code {
  color: var(--accent);
  background-color: var(--primary-gray-20);
  padding-left: 0.5rem;
  padding-right: 0.5rem;
}

ul {
  list-style-type: none;
  padding-left: 0rem;
  padding-inline-start: 0.5rem;
}
ul li {
  list-style-type: none;
  padding-left: 1.0rem;
  position: relative;
}
ul li::before {
  position: absolute;
  left: 0;
  content: "\25a0";
  color: var(--primary);
}

ul.nav-menu {
  display: flex;
  padding-right: 1rem;
}

ul.nav-menu li {
  marker: none;
  list-style-type: none;
}
ul.simple li p {
  margin-block: 0;
}

table, th, td {
  border: 1px var(--primary) solid;
  border-collapse: collapse;
}

th {
  margin-block: 0;
  padding: 0.5rem;
}
th p {
  margin-block: 0;
}

td {
  padding: 0.5rem;
}
td p{
  margin-block: 0;
}

div.header {
  display: flex;
  margin-top: 0;
  margin-bottom: 0;
  padding-top: 0;
  padding-bottom: 0;
  background-color: var(--primary);
  justify-content: space-between;
  align-content: center;
  align-items: center;
  position: sticky;
  top: 0;
  height: 2.25rem;
}
div.header a {
  display: flex;
  align-items: center;
  align-content: center;
  margin-block-start: 0;
  margin-block-end: 0;
  color: var(--white);
  padding-left: 1rem;
  vertical-align: middle;
}
div.header a:hover {
  color: var(--accent);
}
div.header a h3 {
  margin-block-start: 0;
  margin-block-end: 0;
  vertical-align: middle;
  height: 100%;
}
div.header ul {
  display: flex;
  align-items: center;
  align-content: center;
  margin-block-start: 0;
  margin-block-end: 0;
  vertical-align: middle;
  padding-right: 1rem;
}
div.header ul li {
  align-self: center;
}
div.header ul li img{
  padding:0;
  margin:0;
  display: inline;
  vertical-align: middle;
}


div.content {
  margin-bottom: 0;
  height: calc(100lvh - 2.25rem - 1.7rem);
  width: 100lvw;
  display: flex;
  overflow: hidden;
}

div.document {
  padding-left: 2rem;
  padding-right: 2rem;
  overflow-y: auto;
  height: 100%;
  width: calc(100lvw - 38rem);
}

div.sidebar {
  padding-left: 2rem;
  width: 15rem;
  height: 100%;
  overflow-y: visible;
  border-right: 1px solid var(--secondary);
}

div.right-sidebar {
  width: 15rem;
  height: 100%;
  padding-left: 1rem;
  overflow-y: scroll;
  border-left: 1px solid var(--secondary);
  float: right;
  position: fixed;
  right: 0;
}

div.right-sidebar > div > ul {
  font-size: 0.875rem;
  line-height: 1.25rem;
  margin-left: 0rem;
  padding-left: 0rem;
  padding-inline-start: 0;
}
div.right-sidebar > div > ul li {
  margin-left:0;
  padding-left: 0;
  padding-inline-start: 0;
}
div.right-sidebar > div > ul li:before {
  content: ""
}
/* h1 page title */
div.right-sidebar > div > ul > li > a {
  font-weight: bold;
  margin-left:0;
  padding-left: 0;
  padding-inline-start: 0;
}
div.right-sidebar > div > ul > li > ul li:before {
  position: absolute;
  left: -1rem;
  content: "\25a0";
  color: var(--primary);
}
/* h2 page title */
div.right-sidebar > div > ul > li > ul{
  font-size: 0.875rem;
  line-height: 1.25rem;
  margin-left: 1rem;
  padding-left: 0;
  padding-inline-start: 0;
}
/* h3 page title */
div.right-sidebar > div > ul > li > ul > li ul {
  font-size: 0.75rem;
  line-height: 1rem;
  margin-left:0;
  padding-left: 0;
  padding-inline-start: 0;
}
div.right-sidebar > div > ul > li > ul > li ul li:before {
  position: absolute;
  left: -1rem;
  content: "-";
  color: var(--primary);
}

div.footer {
  color: white;
  background-color: var(--primary);
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 1.7rem;
}

button.toggle-button {
  color:white;
}

pre {
  padding: 1rem;
  overflow-x: auto;
}

div.relations {
  display: flex;
  justify-content: space-between;
  margin-top: 2.25rem;
  margin-bottom: 2.25rem;
}

div.relations .arrow-left {
  color: var(--primary);
  position: relative;
  display: inline-block;
  padding-left: 1rem;
}
div.relations .arrow-left:hover {
  color: var(--accent);
}
div.relations .arrow-left:before {
  content: "";
  position: absolute;
  top:0;
  left:0;
  bottom:0;
  margin: auto;
  width: 0.5rem;
  height: 0.5rem;
  border: 0;
  border-top: 3px solid var(--primary);
  border-left: 3px solid var(--primary);
  transform:  rotate(-45deg);
}

div.relations .arrow-right {
  color: var(--primary);
  position: relative;
  display: inline-block;
  padding-right: 1rem;
}
div.relations .arrow-right:hover {
  color: var(--accent);
}
div.relations .arrow-right:before {
  content: "";
  position: absolute;
  top:0;
  right:0;
  bottom:0;
  margin: auto;
  width: 0.5rem;
  height: 0.5rem;
  border: 0;
  border-top: 3px solid var(--primary);
  border-right: 3px solid var(--primary);
  transform:  rotate(45deg);
}

div.documentparents ul{
  display: flex;
  list-style-type: none;
  padding-left: 0rem;
  padding-inline-start: 0;
}
div.documentparents ul li:before {
  content: "Â»";
  padding-left:0.25rem;
}
div.documentparents ul li:first-child {
  padding-left: 0;
}
div.documentparents ul li:first-child:before {
  content: "";
}

div.admonition {
  padding-bottom: 0.5rem;
  border-left: 3px solid var(--primary);
}
div.admonition p.admonition-title {
  background-color: var(--primary);
  color: white;
}
div.admonition table {
  margin: 1rem;
}
div.admonition {
  background-color: var(--white);
}
div.admonition p {
  padding-left: 1rem;
}
div.admonition.note {
  border-left-color: var(--light-blue);
}
div.admonition.note p.admonition-title {
  color: white;
  background-color: var(--light-blue);
}
div.admonition.warning {
  border-left-color: var(--light-yellow);
}
div.admonition.warning p.admonition-title {
  color: white;
  background-color: var(--light-yellow);
}

search#searchbox {
  padding-top: 1rem;
}

.img-light {
  display: inline-block;
  filter: invert(100%);
}
